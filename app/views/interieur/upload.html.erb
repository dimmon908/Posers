<h2>Upload nieuw interieur</h2>
<%= error_messages_for :interieur %>

<%= form_for(:interieur, :html => { :multipart => true }) do |f| -%>
<table id="user-full">
    <tr class="gray">
		<td class="label">Bestand:</td>
		<td><%= f.file_field :image %></td>
	</tr>
    <tr class="noborder">
      <td colspan="2">
        <%= submit_tag "Uploaden", :class => "submit" -%>
      </td>
    </tr>
</table>
<% end -%>